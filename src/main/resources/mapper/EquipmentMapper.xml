<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.forest.mapper.EquipmentMapper">

    <resultMap id="BaseResultMap" type="com.example.forest.model.persist.entity.Equipment">
            <id property="id" column="id" />
            <result property="equipmentName" column="equipment_name" />
            <result property="status" column="status" />
            <result property="useName" column="use_name" />
            <result property="remark" column="remark" />
            <result property="createTime" column="create_time" />
            <result property="updateTime" column="update_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,equipment_name,status,use_name,remark,create_time,
        update_time
    </sql>
</mapper>
